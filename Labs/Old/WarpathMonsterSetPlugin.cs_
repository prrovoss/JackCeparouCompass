    public class WarpathMonsterSetPlugin : BasePlugin
    {
        public IBrush BackgroundBrush { get; set; }
        public IFont TextFont { get; set; }
        public IFont DebugFont { get; set; }

        public WarpathMonsterSetPlugin()
        {
            Enabled = true;
        }

        public override void Load(IController hud)
        {
            base.Load(hud);

            BackgroundBrush = hud.Render.CreateBrush(178, 0, 0, 0, 0);
            TextFont = Hud.Render.CreateFont("arial", 7, 224, 240, 240, 64, true, false, false);
            DebugFont = Hud.Render.CreateFont("arial", 7, 224, 255, 0, 0, true, false, false);

            //lastTick = Hud.CurrentMilliseconds;
        }

        private readonly List<ushort> currentSets = new List<ushort>();
        private readonly List<Tuple<ushort, int>> setsStats = new List<Tuple<ushort, int>>();
        private List<string> debugSnos = new List<string>();
        //private long lastTick;
        private string lastArea;
        public override void PaintTopInGame(ClipState clipState)
        {
            if (clipState != ClipState.BeforeClip) return;
            /**/
            if ((Hud.MySpecialArea != SpecialArea.Rift) && (Hud.MySpecialArea != SpecialArea.GreaterRift))
            {

                return;
            }/**/

            //if (lastTick == 0 || lastTick + 100 < Hud.CurrentMilliseconds || lastTick > Hud.CurrentMilliseconds)
            //{
                var monstersHud = Hud.Collections.GetAliveMonsters().Where(monster => !monster.Invisible && !monster.IsElite);//.Select(monster => monster.Sno).Distinct();

                currentSets.Clear();
                debugSnos.Clear();
                foreach (var monster in monstersHud)
                {
                    if (MonsterList.Items.Any(m => m.Sno == monster.Sno))
                    {
                        currentSets.AddRange(MonsterList.Items.First(m => m.Sno == monster.Sno).Sets);
                    }
                    else
                    {
                        debugSnos.Add(string.Format("{0,6} {1}", monster.Sno, monster.SnoMonsterInfo.Name));
                    }
                }

                debugSnos = debugSnos.Distinct().ToList();
            //    lastTick = Hud.CurrentMilliseconds;
            //}

            var groupedSets = currentSets.GroupBy(i => i).OrderByDescending(i => i.Count());
            var groupedDebugSnos = debugSnos.GroupBy(i => i).OrderByDescending(i => i.Count());

            var y = 120;
            const int x = 20;
            const int lineHeight = 14;

            BackgroundBrush.DrawRectangle(x - lineHeight/2, y-lineHeight/2, 120, lineHeight * (groupedSets.Count() + groupedDebugSnos.Count() + 2) + lineHeight);

            //foreach (var t in Hud.Collections.GetSceneHints())
            //{
            //    TextFont.DrawText(t.Hint, x, y);
            //    y += lineHeight;
            //}

            TextFont.DrawText("Monster Set(s) :", x, y);
            y += lineHeight;

            foreach (var set in groupedSets)
            {
                TextFont.DrawText(string.Format("Set {0,2} : {1}", set.Key, set.Count()), x, y);
                y += lineHeight;
            }

            //6057	Triune_berserker_hGlowSphere
            //5187	Sandling_A	Dust Imp
            //6055	Triune_Berserker_D	Hell Basher
            //178213	TriuneCultist_E	Enraged Zealot
            //418911	p1_LR_Ghost_A	Enraged Phantom
            //5209	sandWasp_B Dune Stinger
            //4152	FleshPitFlyerSpawner_A	Carrion Nest

            if (debugSnos.Count > 0)
            {
                DebugFont.DrawText("Snos not found :", x, y);
                y += lineHeight + 6;
				foreach (var sno in groupedDebugSnos)
				{
					DebugFont.DrawText(string.Format("Sno {0} : {1}", sno.Key, sno.Count()), x, y);
					y += lineHeight;
				}
            }

            // Sno Not Found :
            // ===============
            // Scouring Lobber
            // Warscarred Footsoldier
            // Punisher
            // Returned Executioner
            // Skeletal Executioner
            // Doom Viper
            // Morlu Incinerator
            // Morlu Legionnaire
            // Dark Moon Clan Impaler
