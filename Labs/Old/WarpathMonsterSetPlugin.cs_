    #region WarPath
    public class Monster
    {
        public uint Sno { get; private set; }
        public List<ushort> Sets { get; private set; }

        public Monster(uint sno)
        {
            Sets = new List<ushort>();
            Sno = sno;
        }
    }

    public static class MonsterList
    {
        public static List<Monster> Items = new List<Monster>();

        public static void AddMonster(uint sno, ushort set)
        {
            var monster = Items.FirstOrDefault(m => m.Sno == sno);
            if (monster == null)
            {
                Items.Add(new Monster(sno));
                monster = Items.First(m => m.Sno == sno);
            }
            monster.Sets.Add(set);
        }

